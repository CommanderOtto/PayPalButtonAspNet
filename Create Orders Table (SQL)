CREATE TABLE [dbo].[Orders] (
    [PaypalOrderId]       NVARCHAR (50) NULL,
    [OrderStatus]         NVARCHAR (50) NULL,
    [AccountInfoEmail]    NVARCHAR (50) NULL,
    [AccountHolderName]   NVARCHAR (50) NULL,
    [AccountInfoBirthday] NVARCHAR (50) NULL,
    [PayPalAccountId]     NVARCHAR (50) NULL,
    [PhoneCountryCode]    NVARCHAR (50) NULL,
    [PhoneNumber]         NVARCHAR (50) NULL,
    [PhoneExtension]      NVARCHAR (50) NULL,
    [Address1]            NVARCHAR (50) NULL,
    [Address2]            NVARCHAR (50) NULL,
    [City]                NVARCHAR (50) NULL,
    [State]               NVARCHAR (50) NULL,
    [Country]             NVARCHAR (50) NULL,
    [Postal]              NVARCHAR (50) NULL,
    [ProductId]           NVARCHAR (50) NULL,
    [ProductDescription]  NVARCHAR (50) NULL,
    [ProductQuantity]     NVARCHAR (50) NULL,
    [AmountValue]         NVARCHAR (50) NULL,
    [AmountCurrencyCode]  NVARCHAR (10) NULL,
    [PurchaseDate]        NVARCHAR (50) NULL,
    [PaymentStatus]       NVARCHAR (50) NULL,
    [PaymentId]           NVARCHAR (50) NULL,
    [OrderId]             INT           IDENTITY (1, 1) NOT NULL,
    CONSTRAINT [PK_Orders] PRIMARY KEY CLUSTERED ([OrderId] ASC)
);
